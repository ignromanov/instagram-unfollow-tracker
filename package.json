{
  "name": "instagram-unfollow-tracker",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "description": "Find out who unfollowed you on Instagram â€” 100% local, free, open source unfollow tracker",
  "keywords": [
    "instagram",
    "unfollow",
    "tracker",
    "privacy",
    "local",
    "react",
    "typescript",
    "open-source",
    "data-analysis",
    "social-media"
  ],
  "homepage": "https://ignromanov.github.io/instagram-unfollow-tracker",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ignromanov/instagram-unfollow-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/ignromanov/instagram-unfollow-tracker/issues"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "test": "vitest run --reporter=verbose",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage --reporter=verbose",
    "test:coverage:ui": "vitest --coverage --ui",
    "test:coverage:strict": "vitest run --coverage --reporter=verbose --coverage.thresholds.statements=95 --coverage.thresholds.branches=90 --coverage.thresholds.functions=95 --coverage.thresholds.lines=95",
    "test:coverage:minimal": "vitest run --coverage --reporter=verbose --coverage.thresholds.statements=80 --coverage.thresholds.branches=75 --coverage.thresholds.functions=80 --coverage.thresholds.lines=80",
    "test:ci": "vitest run --coverage --reporter=verbose --reporter=html",
    "test:fast": "vitest run --reporter=default --no-coverage",
    "test:bail": "vitest run --bail=1 --reporter=verbose",
    "test:debug": "vitest run --reporter=verbose --no-coverage --bail=1 --testTimeout=30000",
    "test:performance": "NODE_OPTIONS='--max-old-space-size=4096' vitest run --reporter=verbose --no-coverage",
    "test:strict": "vitest run --reporter=verbose --testTimeout=5000 --bail=1",
    "preview": "vite preview --port 5173",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:strict": "eslint . --max-warnings 0",
    "type-check": "tsc --noEmit --project tsconfig.json",
    "type-check:all": "tsc --noEmit --project tsconfig.json && tsc --noEmit --project tsconfig.test.json",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "code:check": "npm run lint:strict && npm run type-check",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-virtual": "^3.13.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fastbitset": "^0.4.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.544.0",
    "next-themes": "^0.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0",
    "use-debounce": "^10.0.6",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.0",
    "eslint-plugin-react-refresh": "^0.4.23",
    "eslint-plugin-unused-imports": "^4.2.0",
    "fake-indexeddb": "^6.2.2",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.2.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.14",
    "terser": "^5.44.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.8",
    "vitest": "^3.2.4"
  }
}
